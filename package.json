{
  "name": "flow-to-dts",
  "version": "0.0.1",
  "description": "Convert Flow libdefs to Typescript .d.ts.",
  "keywords": [
    "flowtype",
    "typescript",
    "babel-plugin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/burnnat/flow-to-dts.git"
  },
  "author": "Nat Burns",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/burnnat/flow-to-dts/issues"
  },
  "homepage": "https://github.com/burnnat/flow-to-dts#readme",
  "main": "index.js",
  "bin": {
    "flow-to-dts": "./dist/cli.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "watch": "tsc --watch",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build && npm run test",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "commander": "^2.15.1",
    "recast": "^0.15.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.2",
    "@types/node": "^10.3.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-tester": "^5.4.0",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "ts-node": "^6.1.1",
    "typescript": "^2.9.2"
  }
}
